{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node --env-file=.env dist/app.js",
    "prod": "node dist/src/app.js",
    "dev": "tsx watch --env-file=.env src/app.ts",
    "drizzle-kit": "pnpx tsx node_modules/drizzle-kit/bin.cjs",
    "db:generate": "pnpm run drizzle-kit generate",
    "db:migrate": "pnpm run drizzle-kit migrate",
    "db:push": "pnpm run drizzle-kit push"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "drizzle-kit": "^0.21.4",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.11.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.10.1",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.2",
    "mysql2": "^3.9.8",
    "zod": "^3.23.8"
  }
}
